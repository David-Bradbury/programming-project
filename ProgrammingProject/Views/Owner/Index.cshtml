@model Owner
@{
    ViewData["Title"] = "Owner";

}

<div>
    <h1 class="display-4">This is, @Model.FirstName @Model.LastName</h1>
    <h6 class="display-4">@Model.FirstName is an Owner</h6>

    @foreach (var dog in @Model.Dogs)
    {
        <partial name="../Shared/Components/_OwnerDogCard.cshtml" model="dog" />
    }
</div>