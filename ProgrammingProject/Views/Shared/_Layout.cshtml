<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ProgrammingProject</title>
    @*<link rel="stylesheet" href="~/lib/buootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ProgrammingProject.styles.css" asp-append-version="true" />*@
    <link rel="stylesheet" href="~/dist/bundle.css" asp-append-version="true" />
</head>
<body>
    <header>
        <partial name="_navbar" />
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer style="background-color:#429EA6;" class="border-top footer text-muted" >
        <div style="color:white;" class="container">
            &copy; 2023 - ProgrammingProject - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>*@
    <script src="~/dist/bundle.js" asp-append-version="true"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
